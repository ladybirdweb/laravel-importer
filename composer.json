{
    "name": "lws/import",
    "description": ":description",
    "license": "MIT",
    "authors": [
        {
            "name": "Ladybird Web Solution",
            "email": "support@ladybirdweb.com",
            "homepage": "https://ladybirdweb.com"
        }
    ],
    "homepage": "https://github.com/lws/import",
    "keywords": ["Laravel", "Import"],
    "require": {
        "illuminate/support": "~5",
        "jeremeamia/superclosure": "^2.4",
        "opis/closure": "^3.3",
        "spatie/async": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LWS\\Import\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LWS\\Import\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LWS\\Import\\ImportServiceProvider"
            ],
            "aliases": {
                "Import": "LWS\\Import\\Facades\\Import"
            }
        }
    }
}
